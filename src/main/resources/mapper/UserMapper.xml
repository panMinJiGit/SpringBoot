<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cc.springboot.mapper.UserMapper">


    <select id="selectAll" resultType="com.cc.springboot.entity.UserEntity">
        select
            user_id userId,
            device_imei deviceImei,
            device_name deviceName
         from equipment_info
    </select>



</mapper>